{
    "name": "template",
    "type": "module",
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "dev": "solid-start dev --port 3000",
        "start": "solid-start start --port 3000",
        "build": "solid-start build",
        "lint": "eslint \"**/*.{ts,tsx,cjs,mjs,js,jsx}\"",
        "format": "eslint --fix \"**/*.{ts,tsx,cjs,mjs,js,jsx}\"",
        "clean": "rm -rf node_modules package-lock.json",
        "db:push": "prisma db push --schema ./src/server/db/schema.prisma",
        "db:generate": "prisma generate --schema ./src/server/db/schema.prisma",
        "postinstall": "prisma generate --schema ./src/server/db/schema.prisma"
    },
    "dependencies": {
        "@auth/core": "^0.2.5",
        "@auth/prisma-adapter": "^1.0.1",
        "@auth/solid-start": "^0.1.1",
        "@prisma/client": "^5.1.1",
        "@solidjs/meta": "^0.28.6",
        "@solidjs/router": "^0.8.3",
        "@upstash/ratelimit": "^0.4.3",
        "@upstash/redis": "^1.22.0",
        "solid-js": "^1.7.11",
        "solid-start": "^0.2.32",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@types/node": "^20.4.10",
        "@typescript-eslint/eslint-plugin": "^6.3.0",
        "@typescript-eslint/parser": "^6.3.0",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.47.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.1.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.28.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-solid": "^0.12.1",
        "postcss": "^8.4.27",
        "prisma": "^5.1.1",
        "solid-start-node": "^0.2.19",
        "solid-start-vercel": "^0.2.21",
        "tailwindcss": "^3.3.3",
        "typescript": "^5.1.6",
        "vite": "^4.4.9"
    }
}
